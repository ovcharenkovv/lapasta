-#= javascript_include_tag "/javascripts/ckeditor/ckeditor.js"
= semantic_form_for [:admin, @page_category] do |f|
  = f.inputs :name => "inputs" do
    = f.input :published
    = f.input :slug
    = f.input :name
    = f.input :description, :as => :ckeditor, :input_html => { :height => 200 }
    = f.input :meta_title
    = f.input :meta_description
    = f.input :meta_keywords
  = f.buttons